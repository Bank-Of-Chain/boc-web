{
  "name": "boc-web",
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "version": "1.0.0",
  "homepage": ".",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ethersproject/address": "^5.0.5",
    "@ethersproject/providers": "^5.0.12",
    "@gnosis.pm/safe-apps-sdk": "7.0.0",
    "@gnosis.pm/safe-apps-web3modal": "9.0.0",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@reduxjs/toolkit": "^1.7.1",
    "@walletconnect/web3-provider": "^1.7.7",
    "axios": "^0.21.1",
    "bignumber.js": "^9.1.0",
    "classnames": "^2.3.1",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.1",
    "cron-parser": "^4.4.0",
    "ethers": "^5.4.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "piggy-finance-utils": "1.0.59",
    "prop-types": "^15.8.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "swiper": "^8.2.3",
    "web3modal": "1.9.5"
  },
  "devDependencies": {
    "filemanager-webpack-plugin": "^7.0.0",
    "@testing-library/dom": "^6.12.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^12.1.8",
    "@types/react": "^16.9.19",
    "babel-eslint": "^10.1.0",
    "depcheck": "^1.4.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "inquirer": "8.2.0",
    "material-ui-popup-state": "^3.1.1",
    "minimist": "^1.2.6",
    "mustache": "^4.2.0",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^3.4.4",
    "source-map-explorer": "^2.5.2",
    "surge": "^0.21.5",
    "unimported": "^1.21.0"
  },
  "scripts": {
    "apollo": "node ./configs/apollo.js",
    "mustache": "mustache ./configs/address.json ./configs/config.template.js > ./src/config/config.js",
    "start": "set PORT=3001 && react-app-rewired start",
    "build": "react-app-rewired build",
    "apollo:local": "node configs/apollo.js --env=local",
    "apollo:qa-sg": "node configs/apollo.js --env=qa-sg",
    "apollo:qa02-sg": "node configs/apollo.js --env=qa02-sg",
    "apollo:qa03-sg": "node configs/apollo.js --env=qa03-sg",
    "apollo:qa04-sg": "node configs/apollo.js --env=qa04-sg",
    "apollo:stage-sg": "node configs/apollo.js --env=stage-sg",
    "apollo:pr-sg": "node configs/apollo.js --env=pr-sg --chain=configBase",
    "apollo:pr02-sg": "node configs/apollo.js --env=pr02-sg --chain=configBase",
    "eject": "react-scripts eject",
    "test": "react-app-rewired test",
    "lint": "prettier --write .",
    "surge": "surge ./build",
    "ship": "yarn surge",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier -c -w \"src/**/*.{js,jsx}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prepare": "husky install",
    "unimported": "unimported --flow",
    "depcheck": "depcheck"
  }
}